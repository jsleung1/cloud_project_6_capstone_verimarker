div.submission-upload-container{
}
a img {
  border: none;
}
.blue-box {
  margin-bottom: 10px;
  padding: 15px;
  border: #c6dcf1 1px solid;
  border-radius: 10px;
  background-color: #FFF;
}
.notice-message {
  background: #FFFECC none repeat scroll 0 0;
  border: 1px solid #FAECB5;
  padding: 10px;
  margin: 0 0 10px 0;
}
.notice-message h4 {
  color: #BBAB67;
  margin: 0;
}
/* Texts */
h1, h2, h3, h4, h5, h6 {
  font-family: "myriad pro","trebuchet ms","helvetica neue",helvetica,arial,sans-serif;
  font-weight: normal;
  color: #385C69;
}
h1 {font-size: 36px; letter-spacing: 2px;}
h2 {font-size: 24px; letter-spacing: 1px;}
h3 {font-size: 1.5em;}
h4 {font-size: 1.4em;}
p {
  margin-bottom: 1em;
}
li ul, li ol {
  margin: 0 1.5em;
}
ul, ol {
  margin: 0 1.5em 1.5em 1.5em;
}
ul {
  list-style: url('src/assets/images/veriguide-submission-upload/list-dot.png') circle outside;
}
ol {
  list-style-type: decimal;
}
li {
  margin-top: 3px;
  padding-bottom: 2px;
}
strong {
  font-weight: bold;
}
/*Table form-part Items*/
input, select{
  width: 100%;
  padding: 5px;
  border: #c6dcf1 1px solid;
  background-color: #FFF;
  color: black;
  font-size: 13px;
}
button{
  width: 100%;
  padding: 5px;
  border: #c6dcf1 1px solid;
  background-color: #FFF;
  color: black;
  font-size: 15px;
}
button[type=submit]{
  background-color:#dfeffc;
  cursor: pointer;
}
button[type=submit]:hover{
  background-color:#EFF8FF;
}
input[type=radio], input[type=checkbox]{
  width: auto;
}
div.form-part {
  border-collapse: separate;
  border-spacing: 0;
}
div.form-part, div.row, div.form-row {
  vertical-align: middle;
  text-align: left;
  font-weight: normal;
  padding: 4px 0px;
}
div.form-part .fielddesc{
  font-family: "Myriad Pro", Arial, Helvetica, sans-serif;
  color:#385c69;
  font-size: 17px;
  text-align: right;
  padding: 5px 10px 5px 5px;
}
div.form-part .fielddesc span{
  display: block;
  font-size: 10px;
  color:#999;
}
div.form-part .field{
  padding: 5px 20px 5px 10px;
  vertical-align: middle;
}
div.form-part .fielderror{
  padding: 5px 20px 5px 10px;
  color: #8a0000;
  font-size: 12px;
  /*	vertical-align:middle; */
}
div.form-part div.input-group.field {
  padding: 0px;
}
div.form-part div.input-group.field input {
  padding-left: 10px;
}
div.form-part .formdesc{
  font-size: 12px;
  color:#999;
}
div.form-part input.error{
  background: #FFEDEF;
  border-color: #8a0000;
  color: #8a0000;
  border: 3px #8a0000;
}
div.form-part input.valid{
  color:#0b8a00;
  border: 1px solid #89d982;
  background: #e7f7e6;
}
div.form-part tr.form-field td{
  background: #E9F0F7;
}
div.form-part div.form-group {
  margin-top: 0px;
  margin-bottom: 0px;
}
.custom-file-input ~ .custom-file-label::after {
  content: "...";
}
button.calendar, button.calendar:active {
  width: 2.5rem;
  height: 2.25em;
  background-color: #e9ecef;
  background-image: url('src/assets/images/veriguide-submission-upload/datepicker-button-logo.png') !important;
  background-repeat: no-repeat;
  background-size: 20px;
  background-position: center;
}
